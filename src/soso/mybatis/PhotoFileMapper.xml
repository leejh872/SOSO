<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="soso.dao.PhotoFileDao">

<select id="getList" resultType="soso.entities.PhotoFile">      
   SELECT * FROM PHOTOFILES WHERE PHOTO_CODE= #{photoCode}
   </select>
   
   <insert id="insert">
<!--       <selectKey order="BEFORE" keyProperty="code" resultType="String">
      SELECT ISNULL(MAX(CAST(CODE AS INT)),0)+1 AS CODE FROM PHOTOFILES
      </selectKey> -->
      
      INSERT INTO PHOTOFILE(POST_CODE,SRC,PHOTO)
      VALUES(#{postCode},#{src},#{photo})
   </insert>
  
  
</mapper>

